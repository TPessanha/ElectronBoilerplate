{
	"defaultSeverity": "warning",
	"extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
	"jsRules": {},
	"rules": {
		"semicolon": true,
		"object-literal-sort-keys": false,
		"variable-name": [
			true,
			"ban-keywords",
			"check-format",
			"allow-leading-underscore"
		]
	},
	"rulesDirectory": []
}
